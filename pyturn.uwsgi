[uwsgi]
# symlink this under /etc/uwsgi/apps-enabled/ (Debian)
socket = localhost:5678
stats-http
stats-server = localhost:5679
plugin = python3
wsgi-file = /usr/local/jcomeauictx/myturn/myturn.py
log-date = @(exec:///bin/date +%%Y-%%m-%%d:%%H:%%M:%%S)
callable = server
uid = www-data
threads = 4
processes = 1
